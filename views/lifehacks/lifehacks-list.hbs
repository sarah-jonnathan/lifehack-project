<h2>Lifehacks list</h2>
{{#each lifehacksArray}}
    <div>
        <h1>{{this.title}}</h2>
        {{#each tags}}
            <span><a href="/tags/{{this._id}}">{{this.name}}</a></span>
        {{/each}}

        <p>{{this.description}}</p>
        <h3>multimedia: {{this.embedMultimedia}}</h3>
        <a href="/lifehacks/{{this._id}}">more...</a>

    </div>
<hr>
{{/each}}